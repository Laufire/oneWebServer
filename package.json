{
  "name": "@laufire/one-webserver",
  "version": "0.0.0",
  "description": "A simple, declarative node web-server.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest --no-cache",
    "lint": "eslint ./",
    "build": "babel --presets es2015 -d ./dist ./src",
    "link": "npm build && npm link"
  },
  "engines": {
    "node": ">=7.6"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Laufire/oneWebServer"
  },
  "keywords": [
    "simple",
    "declarative",
    "web-server"
  ],
  "author": "Laufire Technologies",
  "license": "ISC",
  "homepage": "https://github.com/Laufire/oneWebServer",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-eslint": "^10.0.2",
    "babel-preset-es2015": "^6.24.1",
    "acorn": "^6.0.0",
    "eslint": "^5.16.0",
    "regenerator-runtime": "^0.13.2",
    "jest": "^24.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "./.jest-coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "@laufire/one-router": "0.0.3",
    "@laufire/utils": "0.0.28",
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  }
}
